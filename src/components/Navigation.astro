---
import '../styles/global.css';

const navItems = [
  {
    label: '首页',
    href: '/',
  },
  {
    label: '关于',
    href: '/about/',
  },
  {
    label: '博客',
    href: '/blog/',
  },
];
---

<div class="nav-links">
  {
    navItems.map((item) => (
      <a href={item.href} class="nav-link">
        {item.label}
      </a>
    ))
  }
</div>
